{
  "name": "flint",
  "version": "0.11.5",
  "description": "A note-taking app that helps you capture ideas, connect them together, and make them part of how you think",
  "main": "./out/main/index.js",
  "author": "Tim Disney",
  "homepage": "https://www.flintnote.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/disnet/flint-note.git"
  },
  "private": true,
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.web.json",
    "typecheck": "npm run typecheck:node && npm run typecheck:web && npm run svelte-check",
    "test": "vitest",
    "test:run": "vitest run",
    "start": "electron-vite preview",
    "check": "npm run lint && npm run typecheck",
    "clean": "rm -rf out dist node_modules/.vite src/server/dist",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build && npm run copy-templates",
    "copy-templates": "cp -r src/server/templates out/",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build:win:production": "npm run build && electron-builder --win --config electron-builder.production.yml",
    "build:mac:production": "npm run build && electron-builder --mac --config electron-builder.production.yml",
    "build:linux:production": "npm run build && electron-builder --linux --config electron-builder.production.yml",
    "build:win:canary": "npm run build && electron-builder --win --config electron-builder.canary.yml",
    "build:mac:canary": "npm run build && electron-builder --mac --config electron-builder.canary.yml",
    "build:linux:canary": "npm run build && electron-builder --linux --config electron-builder.canary.yml",
    "docs:dev": "vitepress dev docs-src",
    "docs:build": "vitepress build docs-src",
    "docs:preview": "vitepress preview docs-src"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/gateway": "^1.0.0",
    "@codemirror/autocomplete": "^6.18.7",
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/search": "^6.5.11",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@types/dompurify": "^3.0.5",
    "ai": "^5.0.0",
    "chokidar": "^4.0.3",
    "dompurify": "^3.2.6",
    "electron-updater": "^6.6.2",
    "js-yaml": "^4.1.0",
    "marked": "^16.1.2",
    "quickjs-emscripten": "^0.31.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@azure/core-client": "^1.10.1",
    "@azure/identity": "^4.13.0",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.2",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^3.1.0",
    "@fsegurai/codemirror-theme-github-dark": "^6.2.0",
    "@fsegurai/codemirror-theme-github-light": "^6.2.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.8",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^22.18.11",
    "@types/sqlite3": "^3.1.11",
    "@vitest/ui": "^3.2.4",
    "codemirror": "^6.0.2",
    "dotenv": "^17.2.3",
    "electron": "^38.2.1",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-svelte": "^3.5.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.26.1",
    "svelte-check": "^4.1.5",
    "tsx": "^4.20.3",
    "vite": "^6.2.6",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  }
}
